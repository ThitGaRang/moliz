<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.modelexecution.fumldebug.debugger.activityProviderFactory">
      <activityProviderFactory
            activityProviderFactoryClass="org.modelexecution.fumldebug.debugger.papyrus.provider.PapyrusActivityProviderFactory">
      </activityProviderFactory>
   </extension>
   <extension
         point="org.eclipse.debug.ui.debugModelPresentations">
      <debugModelPresentation
            class="org.modelexecution.fumldebug.debugger.papyrus.presentation.PapyrusDebugPresentation"
            id="org.modelexecution.fumldebug.debugger.papyrus">
      </debugModelPresentation>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.diagram.ui.decoratorProviders">
      <decoratorProvider
            class="org.modelexecution.fumldebug.debugger.papyrus.presentation.DebugDecoratorProvider">
         <Priority
               name="Highest">
         </Priority>
         <object
               class="org.eclipse.gmf.runtime.notation.Node(org.eclipse.gmf.runtime.notation)"
               id="NODE">
         </object>
         <context
               decoratorTargets="NODE">
         </context>
      </decoratorProvider>
   </extension>

</plugin>
