<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="testlang.presentation.TestlangModelWizardID"
            name="%_UI_TestlangModelWizard_label"
            class="testlang.presentation.TestlangModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/TestlangModelFile.gif">
         <description>%_UI_TestlangModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="testlang.presentation.TestlangEditorID"
            name="%_UI_TestlangEditor_label"
            icon="icons/full/obj16/TestlangModelFile.gif"
            extensions="testlang"
            class="testlang.presentation.TestlangEditor"
            contributorClass="testlang.presentation.TestlangActionBarContributor">
      </editor>
   </extension>

</plugin>
