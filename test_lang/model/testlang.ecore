    <eStructuralFeatures xsi:type="ecore:EReference" name="assertions" upperBound="-1"
        eType="#//Assertion" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="testCase" lowerBound="1"
        eType="#//TestCase" eOpposite="#//TestCase/tests"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ActivityUnderTest">
