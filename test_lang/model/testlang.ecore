    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" lowerBound="1"
        eType="#//TempOperator"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="node" eType="ecore:EClass platform:/plugin/org.eclipse.uml2.uml/model/UML.ecore#//ExecutableNode"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TempOperator">
    <eLiterals name="Initially"/>
    <eLiterals name="After" value="1"/>
