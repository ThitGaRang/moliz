  <eClassifiers xsi:type="kernel:BehavioredEClass" name="PlaceConfiguration">
    <eSuperTypes href="http://petrinet/1.0#//Place"/>
    <eOperations xsi:type="kernel:BehavioredEOperation" name="removeToken" method="//PlaceConfiguration/removeToken.1"/>
    <eOperations xsi:type="kernel:BehavioredEOperation" name="addToken" method="//PlaceConfiguration/addToken.1"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="tokens" lowerBound="1">
      <eType xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eStructuralFeatures>
    <ownedBehavior xsi:type="fUML.Syntax.CommonBehaviors.BasicBehaviors:OpaqueBehavior" name="addToken" specification="//PlaceConfiguration/addToken">
      <body>{&#xD;
      this.tokens = this.tokens + 1;&#xD;
}</body>
      <language>Alf</language>
    </ownedBehavior>
    <ownedBehavior xsi:type="fUML.Syntax.CommonBehaviors.BasicBehaviors:OpaqueBehavior" name="removeToken" specification="//PlaceConfiguration/removeToken">
      <body>{&#xD;
	this.tokens = this.tokens - 1;&#xD;
}</body>
      <language>Alf</language>
    </ownedBehavior>
  </eClassifiers>
  <eClassifiers xsi:type="kernel:BehavioredEClass" name="TransitionConfiguration">
    <eSuperTypes href="http://petrinet/1.0#//Transition"/>
    <eOperations xsi:type="kernel:BehavioredEOperation" name="isEnabled" method="//TransitionConfiguration/isEnabled.1"/>
    <eOperations xsi:type="kernel:BehavioredEOperation" name="fire" method="//TransitionConfiguration/fire.1"/>
    <ownedBehavior xsi:type="fUML.Syntax.CommonBehaviors.BasicBehaviors:OpaqueBehavior" name="fire" specification="//TransitionConfiguration/fire">
      <body>{&#xD;
	for (Place po : this.output) {&#xD;
		let pco : PlaceConfiguration = (PlaceConfiguration)po;&#xD;
		pco.addToken();&#xD;
	}&#xD;
	&#xD;
	for(Place pi : this.input) {&#xD;
		let pci : PlaceConfiguration = (PlaceConfiguration)pi;&#xD;
		pci.removeToken();&#xD;
	}&#xD;
}</body>
      <language>Alf</language>
    </ownedBehavior>
    <ownedBehavior xsi:type="fUML.Syntax.CommonBehaviors.BasicBehaviors:OpaqueBehavior" name="isEnabled" specification="//TransitionConfiguration/isEnabled">
      <ownedParameter name="return" direction="RETURN">
        <eType xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
      </ownedParameter>
      <body>{&#xD;
	let enabled : Boolean = true;&#xD;
	for(Place pi : this.input) {&#xD;
		let pic : PlaceConfiguration = (PlaceConfiguration)pi;&#xD;
		if(pic.tokens == 0) {&#xD;
			enabled = false;&#xD;
		} &#xD;
	}&#xD;
	return enabled;&#xD;
}</body>
      <language>Alf</language>
    </ownedBehavior>
  </eClassifiers>
  <eClassifiers xsi:type="fUML.Syntax.CommonBehaviors.BasicBehaviors:OpaqueBehavior" name="FoundationalModelLibrary.PrimitiveBehaviors.IntegerFunctions.+">
    <ownedParameter name="x" lowerBound="1"/>
    <ownedParameter name="y" lowerBound="1"/>
    <ownedParameter name="result" lowerBound="1" direction="OUT"/>
  </eClassifiers>
  <eClassifiers xsi:type="fUML.Syntax.CommonBehaviors.BasicBehaviors:OpaqueBehavior" name="FoundationalModelLibrary.PrimitiveBehaviors.IntegerFunctions.-">
