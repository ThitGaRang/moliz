      <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" source="//NetConfiguration/Main/%.1" target="//NetConfiguration/Main/Read%20places/object"/>
      <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" source="//NetConfiguration/Main/%.1" target="//NetConfiguration/Main/Call%20run/target"/>
      <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" source="//NetConfiguration/Main/Read%20places/result" target="//NetConfiguration/Main/%.2"/>
      <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ControlFlow" source="//NetConfiguration/Main/Expansion%20Region" target="//NetConfiguration/Main/Call%20run"/>
      <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ControlFlow" source="//NetConfiguration/Main/Read%20places" target="//NetConfiguration/Main/Expansion%20Region"/>
    </ownedBehavior>
    <ownedBehavior xsi:type="fUML.Syntax.Activities.IntermediateActivities:Activity" name="run" specification="//NetConfiguration/run">
      <node xsi:type="fUML.Syntax.Activities.IntermediateActivities:InitialNode" outgoing="//NetConfiguration/run.1/%.4"/>
      <node xsi:type="fUML.Syntax.Activities.IntermediateActivities:MergeNode" outgoing="//NetConfiguration/run.1/%.5" incoming="//NetConfiguration/run.1/%.4 //NetConfiguration/run.1/%.12"/>
      <node xsi:type="fUML.Syntax.Actions.IntermediateActions:ReadSelfAction" name="Read self" incoming="//NetConfiguration/run.1/%.5">
        <result name="result" outgoing="//NetConfiguration/run.1/%.6" lowerBound="1"/>
      </node>
      <node xsi:type="fUML.Syntax.Actions.IntermediateActions:ReadStructuralFeatureAction" name="Read transitions" outgoing="//NetConfiguration/run.1/%.13">
        <structuralFeature xsi:type="ecore:EReference" href="http://petrinet/1.0#//Net/transitions"/>
        <object name="object" incoming="//NetConfiguration/run.1/%.6" lowerBound="1"/>
        <result name="result" outgoing="//NetConfiguration/run.1/%.7" upperBound="0"/>
      </node>
      <node xsi:type="fUML.Syntax.Activities.ExtraStructuredActivities:ExpansionRegion" name="Expansion Region" outgoing="//NetConfiguration/run.1/%.11" incoming="//NetConfiguration/run.1/%.13" inputElement="//NetConfiguration/run.1/%.2" outputElement="//NetConfiguration/run.1/%.3">
        <node xsi:type="fUML.Syntax.Activities.IntermediateActivities:ForkNode" outgoing="//NetConfiguration/run.1/Expansion%20Region/%.2 //NetConfiguration/run.1/Expansion%20Region/%.4" incoming="//NetConfiguration/run.1/Expansion%20Region/%.1"/>
        <node xsi:type="fUML.Syntax.Actions.BasicActions:CallOperationAction" name="Call isEnabled" operation="//TransitionConfiguration/isEnabled">
          <result name="return" outgoing="//NetConfiguration/run.1/Expansion%20Region/run_decisionInputFlow"/>
          <target name="target" incoming="//NetConfiguration/run.1/Expansion%20Region/%.4" lowerBound="1"/>
        </node>
        <node xsi:type="fUML.Syntax.Activities.IntermediateActivities:DecisionNode" name="isEnabledDecision" outgoing="//NetConfiguration/run.1/Expansion%20Region/%.3" incoming="//NetConfiguration/run.1/Expansion%20Region/%.2 //NetConfiguration/run.1/Expansion%20Region/run_decisionInputFlow" decisionInputFlow="//NetConfiguration/run.1/Expansion%20Region/run_decisionInputFlow"/>
        <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" source="//NetConfiguration/run.1/%.2" target="//NetConfiguration/run.1/Expansion%20Region/%"/>
        <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" source="//NetConfiguration/run.1/Expansion%20Region/%" target="//NetConfiguration/run.1/Expansion%20Region/isEnabledDecision"/>
        <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" source="//NetConfiguration/run.1/Expansion%20Region/isEnabledDecision" target="//NetConfiguration/run.1/%.3">
          <guard xsi:type="kernel:LiteralBoolean" value="true"/>
        </edge>
        <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" source="//NetConfiguration/run.1/Expansion%20Region/%" target="//NetConfiguration/run.1/Expansion%20Region/Call%20isEnabled/target"/>
        <edge xsi:type="fUML.Syntax.Activities.IntermediateActivities:ObjectFlow" name="run_decisionInputFlow" source="//NetConfiguration/run.1/Expansion%20Region/Call%20isEnabled/return" target="//NetConfiguration/run.1/Expansion%20Region/isEnabledDecision"/>
